<!DOCTYPE html>
<html>
    <body>
        <table border='1'>
            <tr>
                <th>Name</th><th>Author</th><th>Year</th><th>Genre</th>
            </tr>
            {% for Book in Books %}
                <tr>
                    <td><a href='{{ path('bookInfo', {'bookId': Book.getId()}) }}'> {{ Book.GetName() }} </a></td>
                    <td> {{ Book.getAuthor() }} </td><td> {{ Book.getAuthor() }} </td><td> {{ Book.getGenre() }} </td>
                </tr>
            {% endfor %}
        </table>
    </body>
</html>